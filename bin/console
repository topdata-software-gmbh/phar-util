#!/usr/bin/env php
<?php

require_once __DIR__ . '/../vendor/autoload.php';

use Symfony\Component\Console\Application;

$application = new Application('PHAR Tool', '1.0.0');
$application->add(new \PharTool\Command\AnalyzePharCommand());
$application->add(new \PharTool\Command\ExtractPharCommand());
$application->add(new \PharTool\Command\RepackPharCommand());
$application->run();
